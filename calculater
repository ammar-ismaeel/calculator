num1 = float(input("enter the first number"))
operator = (input("enter operator"))
num2= float(input("inter the second name"))

if operator == "+" :
    print(num1 + num2)
elif operator == "-" :
    print(num1 - num2)

elif operator == "/" :
    print(num1 / num2)

elif operator == "*" :
    print(num1 * num2)
else:
    print("wrong")